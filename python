list methods
list =["apple","mango","banana","mango","papaya","aaa"]
list.append("pineapple")
list.insert(0,"watermelon")
print(list)

list.remove("papaya")
list.pop()
print(list)

list.sort()
print(list)
list.reverse()
print(list)

list2=list.copy()
print(list2)
list2.clear()
print(list2)

print(list)
print(list.index("mango"))
print(list.count("mango"))

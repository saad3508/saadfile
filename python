print (c)
print(d)
c.add("newadd")
d.add("newadd")


slicing
a="           _______revaTuRe is located in chennai______________________       "
print(a)
print(a[1:4].upper())
print(a.title())
print(a.capitalize())
print(a.swapcase())
print(a.isalpha())
print(a.strip())
print(a.lstrip())
print(a.rstrip())
print(a.lstrip("_"))# Removes leading underscores
print(a.strip(" _"))# Removes leading & trailing underscores


